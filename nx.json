{
  "npmScope": "hsbc",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "app-one": {
      "tags": []
    },
    "app-one-e2e": {
      "tags": [],
      "implicitDependencies": ["app-one"]
    },
    "app-two": {
      "tags": []
    },
    "app-two-e2e": {
      "tags": [],
      "implicitDependencies": ["app-two"]
    },
    "appthree": {
      "tags": []
    },
    "appthree-e2e": {
      "tags": [],
      "implicitDependencies": ["appthree"]
    },
    "scp": {
      "tags": []
    },
    "scp-e2e": {
      "tags": [],
      "implicitDependencies": ["scp"]
    },
    "scp2": {
      "tags": []
    },
    "scp2-e2e": {
      "tags": [],
      "implicitDependencies": ["scp2"]
    },
    "ui": {
      "tags": []
    },
    "ui-e2e": {
      "tags": [],
      "implicitDependencies": ["ui"]
    }
  }
}
